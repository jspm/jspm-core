{
  "name": "@jspm/core",
  "scripts": {
    "build": "npm run build-nodelibs && npm run build-nodelibs-deno",
    "build-nodelibs": "jspm build -ic -d dist/nodelibs ./nodelibs/@empty.dew.js ./nodelibs/dgram.js ./nodelibs/os.js ./nodelibs/sys.js ./nodelibs/@empty.js ./nodelibs/dns.js ./nodelibs/path.js ./nodelibs/timers.js ./nodelibs/domain.js ./nodelibs/process-production.js ./nodelibs/tls.js ./nodelibs/assert.js ./nodelibs/events.js ./nodelibs/process.js ./nodelibs/tty.js ./nodelibs/buffer.js ./nodelibs/fs-promises.js ./nodelibs/punycode.js ./nodelibs/url.js ./nodelibs/child_process.js ./nodelibs/fs.js ./nodelibs/querystring.js ./nodelibs/util.js ./nodelibs/cluster.js ./nodelibs/http.js ./nodelibs/readline.js ./nodelibs/vm.js ./nodelibs/console.js ./nodelibs/https.js ./nodelibs/repl.js ./nodelibs/zlib.js ./nodelibs/constants.js ./nodelibs/module.js ./nodelibs/stream.js ./nodelibs/crypto.js ./nodelibs/net.js ./nodelibs/string_decoder.js",
    "build-nodelibs-deno": "jspm build -ic -d dist/nodelibs-deno ./nodelibs-deno/buffer.ts ./nodelibs-deno/crypto.ts ./nodelibs-deno/fs-promises.ts ./nodelibs-deno/fs.ts ./nodelibs-deno/os.ts ./nodelibs-deno/process.ts ./nodelibs-deno/timers.ts ./nodelibs-deno/tty.ts"
  },
  "type": "module",
  "devDependencies": {
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.2",
    "console-browserify": "^1.2.0",
    "constants-browserify": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "domain-browser": "^4.19.0",
    "events": "^3.2.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "punycode": "^2.1.1",
    "querystring": "^0.2.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.1.1",
    "string_decoder": "^1.3.0",
    "timers-browserify": "^2.0.12",
    "tty-browserify": "^0.0.1",
    "url": "^0.11.0",
    "util": "^0.12.3",
    "vm-browserify": "^1.1.2"
  },
  "exports": {
    "./nodelibs/*": {
      "deno": "./nodelibs-deno/*.ts",
      "default": "./nodelibs/*.js"
    },
    "./nodelibs/_*": null,
    "./nodelibs/assert": "./nodelibs/assert.js",
    "./nodelibs/child_process": "./nodelibs/child_process.js",
    "./nodelibs/events": "./nodelibs/events.js",
    "./nodelibs/fs/promises": {
      "deno": "./nodelibs-deno/fs-promises.ts",
      "default": "./nodelibs/fs-promises.js"
    },
    "./nodelibs/module": "./nodelibs/module.js",
    "./nodelibs/path": "./nodelibs/path.js",
    "./nodelibs/process": {
      "deno": "./nodelibs-deno/process.ts",
      "production": "./nodelibs/process-production.js",
      "default": "./nodelibs/process.js"
    },
    "./nodelibs/readline": "./nodelibs/readline.js",
    "./nodelibs/stream": "./nodelibs/stream.js",
    "./nodelibs/url": "./nodelibs/url.js",
    "./nodelibs/util": "./nodelibs/util.js"
  }
}
