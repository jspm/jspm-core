    "./nodelibs/@empty.dew": "./nodelibs/@empty.dew.js",
    "./nodelibs/chunk-*": null,
    "./nodelibs/dns": "./nodelibs/dns.js",
    "./nodelibs/http": "./nodelibs/http.js",
    "./nodelibs/https": "./nodelibs/https.js",
    "./nodelibs/net": "./nodelibs/net.js",
